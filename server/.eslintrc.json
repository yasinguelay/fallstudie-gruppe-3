{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "no-unused-vars": [
            "error",
            {
            //   Ignore arguments starting with _
              "argsIgnorePattern": "^_"
            }
        ]
    }
}
